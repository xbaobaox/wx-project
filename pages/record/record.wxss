@import "../../weui/weui-wxss/dist/style/weui.wxss";
.record_title {
  padding: 0 0 0 30rpx;
  height: 60rpx;
  color: rgb(163, 163, 163);
  font-size: 26rpx;
  line-height: 60rpx;
  margin: 10rpx 0;
}

.chosen {
  margin: 10 0 10 0rpx;
  height: 100rpx;
  line-height: 100rpx;
  font-size: 36rpx;
  padding: 0 0 0 30rpx;
  background: #fff;
}

.chosen_title {
  padding: 0 0 0 30rpx;
  height: 60rpx;
  color: rgb(163, 163, 163);
  font-size: 26rpx;
  line-height: 70rpx;
}

.chosen_content {
  background: #fff;
  font-size: 26rpx;
  color: rgb(114, 114, 114);
  white-space: pre-wrap;
}
.chosen_content_icon {
  width: 24rpx;
  height: 24rpx;
  position: absolute;
  right: 5%;
  top: 0;
  bottom: 0;
  margin: auto 0;
}
.dialog_title{
  color: #333;
  font-size: 32rpx;
  margin-top:30rpx;
  text-align: left;
  padding:0 30rpx;
}
.chosen_dialog {
  margin-top:40rpx;
  margin-bottom:80rpx;
  display: flex;
  flex-direction: column;
  align-items: flex-start;
  align-content: flex-start;
  color: #666;
  padding: 0 0 0 70rpx;
 
  font-size: 30rpx;
}
.chosen_dialog view{
  margin-bottom:20rpx ;
  display: inline-block
}
